<?xml version="1.0" encoding="UTF-8"?>
<configuration status="DEBUG">
    <appenders>
        <File name="File" fileName="app.log">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} since %r [%t] %-5p %c ------ %msg%n" />
        </File>
    </appenders>
    <loggers>
        <root level="info">
            <appender-ref ref="File" />
        </root>
        <root level="error">
            <appender-ref ref="File" />
        </root>
    </loggers>
</configuration>